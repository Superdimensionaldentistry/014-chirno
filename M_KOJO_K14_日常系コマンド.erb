			CASE 1
				CALL COLORMESSAGE14("　は, はくちゅん!",C_GREEN,1,0)
				PRINTFORMW 「ふ...이 몸の近くにいると凍傷するぜ」
			CASE 0
				PRINTFORML 「ところでお前は誰だ?」
				CALL COLORMESSAGE14("(まだ名前覚えてもらえてない......）",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「...さっきからやけにつっかかってくるな?」
				CALL COLORMESSAGE14("　ご, ごめんなさい!",C_GREEN,2,0)
		ENDSELECT
	ELSEIF CFLAG:318 == 1
		PRINTFORML 「.........」
	ELSEIF CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 2 || CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 202
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「アイス買いに来たの?1本500円ね」
				IF MONEY < 500
					CALL COLORMESSAGE14("　あう...お金足りないよぉ....",C_GREEN,2,0)
				ELSE
					CALL COLORMESSAGE14("　おいしそうなアイス....お金はあるから買っちゃおかな?",C_GREEN,2,0)
				ENDIF
			CASE 0
				PRINTFORML 「今仕事中なんだ.忙しすぎて目が回りそうだよ」
				PRINTFORMW 「人気者は辛いなー」
				CALL COLORMESSAGE14("（とても忙しそう....手伝ってあげた方がいいのかな?)",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「太陽のあん畜生が이 몸とアイスを溶かそうとしてる...」
				PRINTFORMW 「うおー!負けるもんか!」
				CALL COLORMESSAGE14("　がんばって치르노짱!",C_GREEN,2,0)
		ENDSELECT
	;ELSEIF TALENT:연모 
		;PRINTFORML 
	ELSEIF TALENT:사모 || CFLAG:14:1002 == 1
		SELECTCASE RAND:8
			CASE 6
				PRINTFORML 「凶器はナイフより氷の方が優れているわ!」
				PRINTFORML 「氷のナイフは溶けてしまえば証拠が隠滅するからね」
				CALL COLORMESSAGE14("　치르노짱.もしかして外の世界の探偵漫画を読んだ?",C_GREEN,1,0)
				CALL COLORMESSAGE14("「なんだ.다이짱も読んでたのかぁ」",C_AQUA,2,0)
			CASE 5
				CALL COLORMESSAGE14("「다이짱って何の妖精なの?」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　え?私?私は......何の妖精なんだろう.",C_GREEN,1,0)
				CALL COLORMESSAGE14("「ラルバみたいな蝶の妖精?」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　でも冬眠とかしないよ....うーん私っていったい.",C_GREEN,1,0)
				PRINTFORMW 「...なんとなく何か異変が起きそうな予感」
				CALL COLORMESSAGE14("　私の正体で異変なんか起きないよぉ.",C_GREEN,2,0)
			CASE 4
				PRINTFORMW 「また日焼けしないかなー.最近の太陽は軟弱でいかん」
				CALL COLORMESSAGE14("　あの時の치르노짱, とても強くてかっこよかったよ.",C_GREEN,2,0)
			CASE 3
				PRINTFORMW 「鴉天狗には注意したほうがいいぞ.あることないこと記事にされるんだから!」
				CALL COLORMESSAGE14("　大蝦蟇さんの記事こと...?",C_GREEN,1,0)
				CALL COLORMESSAGE14("「そう!あの新聞はうそっぱち.あいつは大蝦蟇相手にもっと善戦したんだ」",C_AQUA,2,0)
			CASE 2
				PRINTFORML 「蛙でも居ないかなぁ」
				CALL COLORMESSAGE14("　また蛙を凍らせるの?可哀想だからやめようよ....",C_GREEN,1,0)
				CALL COLORMESSAGE14("「짱と蘇生させるから大丈夫だって」",C_AQUA,2,0)
			CASE 1
				PRINTFORML 「あーなんかヒマだなー.また異変でも起きないかなー」
				CALL COLORMESSAGE14("　平和なのが一番だよ치르노짱.",C_GREEN,1,0)
				PRINTFORMW 「이 몸にとっちゃ大手振って遊べる異変が起きた時の方が毎日楽しいよ」
			CASE 0
				PRINTFORML 「草原や砂利道で辺りを凍らして寝っ転がると楽しいよ」
				PRINTFORMW 「ツルツル滑って転がれて...もーサイコー!」
			CASEELSE
				PRINTFORML 「ほら, 花びらを凍らすとすごいよ!」
				CALL COLORMESSAGE14("　ドライフラワーにするの?",C_GREEN,1,0)
				PRINTFORMW 「こうやって凍らせば凶器になるわ!」
				CALL COLORMESSAGE14("　わ, わぁ....",C_GREEN,2,0)
		ENDSELECT
	ELSE
		SELECTCASE RAND:6
			CASE 4
				PRINTFORML 「이 몸は夏の日の晴れた時だけ, 人間の里でアイスを売ってるのさ」
				PRINTFORMW 「アイスを真夏の太陽の下で売るなんて芸当は이 몸ぐらしかできないから大儲けだ!」
				CALL COLORMESSAGE14("　私もアイス大好きなんです.もし見かけたら買わせてくださいね.",C_GREEN,2,0)
				SIF DAY:2 == 4
					CALL COLORMESSAGE14("　冬のアイスもおいしそうですけど, 風邪ひいちゃいそう....",C_GREEN,2,0)
			CASE 3
				PRINTFORML 「神社の近くにいる三妖精はすごいぞ」
				PRINTFORML 「サニーは気配を見えなくしルナは光を消せて, スターは...なんと音を感知できるんだ!」
				PRINTFORMW 「悔しいけど, あいつらでかくれんぼで右にでる奴はいないね」
				CALL COLORMESSAGE14("（あれ...何か違うような?)",C_GREEN,2,0)
			CASE 2
				IF DAY:2 == 4
					PRINTFORMW 「冬は退屈だなぁ.何にもしなくても周りは凍ってるし」
					CALL COLORMESSAGE14("　虫や夏の妖精の娘達も冬眠して遊べないから, 冬はちょっと寂しいですよね",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「さーて今日は何を凍らせてやろうかな」
					CALL COLORMESSAGE14("　わ, 私は凍らせないでくださいね....",C_GREEN,2,0)
				ENDIF
			CASE 1
				PRINTFORML 「ところであんたの名前ってなんだっけ?」
				CALL COLORMESSAGE14("　私の名前ですか?私は...",C_GREEN,1,0)
				PRINTFORMW 「覚えるの面倒だから「あんた」のままでいいかな」
				CALL COLORMESSAGE14("　そんなぁ....",C_GREEN,2,0)
			CASE 0
				PRINTFORML 「なんか大人しそうなやつだなー.ご飯食べ忘れたの?」
				CALL COLORMESSAGE14("　ちゃ, 짱と食べてますよ",C_GREEN,1,0)
				IF TALENT:MASTER:가슴사이즈 > -1
					CALL COLORMESSAGE14("「...じゃないとそんなに大きくならないか」",C_AQUA,1,0)
					CALL COLORMESSAGE14("　あ, あの...どこを見てるんですか?",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「本当?草ばかり食べてるんじゃない?」
				ENDIF
			CASEELSE
				PRINTFORML 「あんたはかくれんぼ知ってる?」
				CALL COLORMESSAGE14("　鬼が隠れている子を見つける遊び...ですよね?よく太陽の畑の娘達と遊んでます.",C_GREEN,1,0)
				PRINTFORML 「湖の妖精の間でも大流行中なんだ」
				PRINTFORML 「しかもいつも이 몸が一人勝ちなのさ.エヘン!」
				PRINTFORML 「そうだ!今度やる時はあんたも誘ってあげる」
				CALL COLORMESSAGE14("　私に치르노さんを見つけられるかな....がんばりますね.",C_GREEN,2,0)
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
