	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;절정
	;기입체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 
	RETURN 1
	ENDIF
