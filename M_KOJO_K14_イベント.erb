;==================================================
;일하는중
;==================================================
@M_KOJO_EVENT_K14_13(ARG,ARG:1)
;구상주는 TARGET
;ARG:1에게 남아 있는 일량
;--------------------------------------------------
;기입체크 (=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;일 개시 (직장으로 이동한다)
	;기입체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1

		IF IS_WEEKEND(DAY)
			;土日は三妖精の家で遊ぶ
				PRINTFORML 「あ!今日は三妖精のやつらと遊ぶ約束してたんだ」
				PRINTFORMW 「今日こそギャフンと言わせてやる!」
		ELSEIF TIME:5 == 8
		;朝起きて, 雪が降っていると湖畔で遊ぶ
			PRINTFORML 「雪だあー!!遊ぶぞおー!!」
			PRINTFORMW 「喜んで庭を駆け回るのは, 犬じゃなく氷精の専売特許だ!」
		ELSEIF (DAY:2 == 2 && TIME:5 < 2)
		;夏晴れ晴天なら人間の里でアイス屋
			PRINTFORML 「うぅ～...この暑さ, これは...アイスが欲しくなるね」
			PRINTFORMW 「こうしちゃいられない.さっそくアイスを売りにいかないと」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;업무중
	;기입체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:318 == 1
			PRINTFORMW 
		ELSEIF CFLAG:MASTER:현재위치 == 5 || CFLAG:MASTER:현재위치 == 32
		;土日は三妖精の家で遊ぶ
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「よぉし이 몸についてこい!」
				CASE 0
					PRINTFORML 「次は이 몸がヒーロー役ね!」
				CASEELSE
					PRINTFORML 「今度は何で遊ぶんだ?」
			ENDSELECT
		ELSEIF CFLAG:MASTER:현재위치 == 343
		;朝起きて, 雪が降っていると湖畔で遊ぶ
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「せっかくだから家のかまくらを増築しようかな...」
				CASE 0
					PRINTFORML 「アイスバーンにして誰か転ばせてやろーっと」
				CASEELSE
					PRINTFORML 「雪やこんこん♪キツネもコンコン♪ふってもふっても腹減りやまぬ♪」
					PRINTFORML 「イ～ヌはヨロヨロ♪にわとりコケコ♪ネコは玉食って丸くなる♪」
			ENDSELECT
		ELSEIF CFLAG:MASTER:현재위치 == 2 || CFLAG:MASTER:현재위치 == 202
		;夏晴れ晴天なら人間の里でアイス屋
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「アイスーアイスはいらんかねー」
					PRINTFORML 「치르노印の最強アイスだよー」
				CASE 0
					PRINTFORML 「う"ー...あち"ー...」
					PRINTFORML 「アイスおいしいー...」ﾍﾟﾛﾍﾟﾛ
				CASEELSE
					PRINTFORML 「当たりがでたらもう１本だよー」
					
			ENDSELECT
		ENDIF

		RETURN 1
	ENDIF
	;-------------------------------------------------
	;일 종료
	;기입체크 (=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 2 || CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 202
			PRINTFORMW 「今日はこれで閉店だよ!」
		ELSE
			PRINTFORMW 「あー楽しかった!」
		RETURN 1
		ENDIF
	ENDIF
	;그외
	PRINTFORML  
ENDIF
RETURN 1

